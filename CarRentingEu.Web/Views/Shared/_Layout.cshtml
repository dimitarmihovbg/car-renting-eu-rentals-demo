<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - devdap_cardealer</title>
    <!-- Favicon -->
    <link rel="icon" href="~/images/favicon.ico">

    <environment include="Development">

        <!-- Core Stylesheet -->
        <link rel="stylesheet" href="~/css/site.css" />
        <!-- Responsive CSS -->
        <link rel="stylesheet" href="~/css/responsive.css" />
    </environment>
    <environment exclude="Development">

        <!-- Core Stylesheet -->
        <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
        <!-- Responsive CSS -->
        <link rel="stylesheet" href="~/css/responsive.css" asp-append-version="true" />

    </environment>

    
</head>
<body>
    <div id="loading">
        <div id="loading-center">
            <img src="~/images/loader.gif" alt="">
        </div>
    </div>

    <header id="header" class="defualt">
        <div class="menu">
            <nav id="menu" class="mega-menu">
                <section class="menu-list-items">
                    <div class="container">
                        <div class="row">
                            <div class="col-lg-12 col-md-12">
                                <ul class="menu-logo">
                                    <li>
                                        <a href="http://CaRenting.Eu"><img id="logo_img" src="~/images/logo-light3.png" alt="logo"> </a>
                                    </li>
                                </ul>
                                <ul class="menu-links">
                                    <li><a asp-area="" asp-controller="Home" asp-action="About">About the Project</a></li>
                                    <li><a asp-area="" asp-controller="Home" asp-action="Contact">Contact Me</a></li>
                                    <li><a asp-area="" asp-controller="Rentals" asp-action="Index">Rentals</a></li>                                    
                                    <li><a asp-area="" asp-controller="Cars" asp-action="Index">Cars Available</a></li>
                                    <li><a asp-area="" asp-controller="Customers" asp-action="Index">Customers</a></li>
                                    <li>@await Html.PartialAsync("_LoginPartial")</li>
                                </ul>                                
                            </div>
                        </div>
                    </div>
                </section>
            </nav>
        </div>
    </header>


    @RenderBody()

